\section{Planteamiento}
\begin{frame}[allowframebreaks]{Descripción}
\hypertarget{www1}{}

\AddToShipoutPictureFG*{%
	\AtTextLowerRight{\hspace*{-.9in}\raisebox{-0.1\baselineskip}{
